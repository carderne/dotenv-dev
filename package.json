  },
  "homepage": "https://github.com/carderne/dotenv-dev",
  "devDependencies": {
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "minimist": "^1.2.8"
  }
}
